[PrefixDeclaration]
xsd:		http://www.w3.org/2001/XMLSchema#
owl:		http://www.w3.org/2002/07/owl#
owl2xml:	http://www.w3.org/2006/12/owl2-xml#
quest:		http://obda.org/quest#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
strdf:		http://www.semanticweb.org/ontologies/2011/4/TemporalMeeting.owl#

[SourceDeclaration]
sourceUri	endpoint
connectionUrl	jdbc:postgresql://localhost:5432/endpoint
username	postgres
password	postgres
driverClass	org.postgresql.Driver

[MappingDeclaration] @collection [[

mappingId	Temporal-Meeting
target		strdf:Meeting/{id} a strdf:Meeting ; strdf:hasId {id} ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} a strdf:Period ; strdf:hasSerialization {duration}^^strdf:period .
source		select id, duration from meeting

]]

