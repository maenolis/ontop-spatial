[PrefixDeclaration]
xsd:		http://www.w3.org/2001/XMLSchema#
owl:		http://www.w3.org/2002/07/owl#
owl2xml:	http://www.w3.org/2006/12/owl2-xml#
quest:		http://obda.org/quest#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
strdf:		http://www.semanticweb.org/ontologies/2011/4/TemporalMeeting.owl#

[SourceDeclaration]
sourceUri	endpoint
connectionUrl	jdbc:postgresql://hostPortPlaceHolder/dbNamePlaceHolder
username	usernamePlaceHolder
password	passwordPlaceHolder
driverClass	org.postgresql.Driver

[MappingDeclaration] @collection [[

mappingId	Temporal-Meeting
target		strdf:Meeting/{id} a strdf:Meeting ; strdf:hasId {id} ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} a strdf:Period ; strdf:hasSerialization {duration}^^strdf:period .
source		select id, duration from meeting

mappingId   Temporal Event
target      strdf:Event/{id} a strdf:Event ; strdf:hasId {id} ; strdf:hasTimePropagated {time_propagated}^^xsd:dateTime ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} strdf:hasSerialization {duration}^^strdf:period .
source      select id, time_propagated, duration from event

mappingId   Temporal FetchTable1
target      strdf:FetchTable1/{id} a strdf:FetchTable1 ; strdf:hasId {id} ; strdf:hasName {name} ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} strdf:hasSerialization {duration}^^strdf:period .
source      select id, name, duration from fetch_table_1

mappingId   Temporal FetchTable2
target      strdf:FetchTable2/{id} a strdf:FetchTable2 ; strdf:hasId {id} ; strdf:hasName {name} ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} strdf:hasSerialization {duration}^^strdf:period .
source      select id, name, duration from fetch_table_2

mappingId   Temporal FetchTable3
target      strdf:FetchTable3/{id} a strdf:FetchTable3 ; strdf:hasId {id} ; strdf:hasName {name} ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} strdf:hasSerialization {duration}^^strdf:period .
source      select id, name, duration from fetch_table_3

mappingId   Temporal FetchTable4
target      strdf:FetchTable4/{id} a strdf:FetchTable4 ; strdf:hasId {id} ; strdf:hasName {name} ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} strdf:hasSerialization {duration}^^strdf:period .
source      select id, name, duration from fetch_table_4

mappingId   Temporal FetchTable5
target      strdf:FetchTable5/{id} a strdf:FetchTable5 ; strdf:hasId {id} ; strdf:hasName {name} ; strdf:hasPeriod strdf:Period/{id} . strdf:Period/{id} strdf:hasSerialization {duration}^^strdf:period .
source      select id, name, duration from fetch_table_5
]]

